{
  "name": "low-radical",
  "version": "0.3.0",
  "description": "low-radical",
  "private": true,
  "main": "src/scripts/background.js",
  "repository": "https://github.com/lowwebtech/low-radical.git",
  "author": "lowwebtech <vico@lowweb.tech>",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "npm run dev:firefox",
    "dev:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch --mode=development",
    "dev:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch --mode=development",
    "dev:opera": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=opera webpack --watch --mode=development",
    "build:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack --mode=production",
    "build:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack --mode=production",
    "build:opera": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=opera webpack --mode=production",
    "build": "npm run build:chrome && npm run build:firefox && npm run build:opera",
    "lint": "run-s prettier:fix eslint:fix",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "prettier": "prettier src",
    "prettier:fix": "npm run prettier -- --write",
    "release": "run-s lint build test",
    "release:all": "run-s lint build test release:chrome release:firefox",
    "release:firefox": "web-ext-submit --source-dir extension/firefox",
    "release:chrome": "webstore upload --source=extension/chrome --auto-publish",
    "start": "npm run dev:chrome",
    "test": "jest",
    "test:src": "jest src"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "jest-webextension-mock",
      "dotenv/config"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17",
    "@babel/plugin-proposal-object-rest-spread": "^7.17",
    "@babel/plugin-transform-destructuring": "^7.17",
    "@babel/plugin-transform-runtime": "^7.17",
    "@babel/preset-env": "^7.17",
    "@babel/runtime": "^7.17",
    "autoprefixer": "^10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chrome-webstore-upload-cli": "^2.1",
    "clean-webpack-plugin": "^4.0",
    "copy-webpack-plugin": "^11.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7",
    "eslint": "^8.15",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-jest": "^26.1",
    "eslint-plugin-jsx-a11y": "^6.5",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0",
    "eslint-plugin-promise": "^6.0",
    "eslint-plugin-standard": "^5.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.1",
    "html-loader": "^3.1",
    "html-webpack-plugin": "^5.5",
    "jest": "^28.1",
    "jest-puppeteer": "^6",
    "jest-transform-stub": "^2.0.0",
    "jest-webextension-mock": "^3.7",
    "node-sass": "^7.0",
    "npm-run-all": "^4.1.5",
    "pixelmatch": "^5.3",
    "postcss-loader": "^6.2",
    "postcss-preset-env": "^7.5",
    "prettier": "^2.6",
    "puppeteer": "^14.0",
    "resolve-url-loader": "^5.0",
    "sass-loader": "^12.6",
    "terser-webpack-plugin": "^5.3",
    "web-ext-submit": "^6.8",
    "webpack": "^5.58",
    "webpack-cli": "^4.9",
    "webpack-dev-server": "^4.9",
    "webpack-fix-style-only-entries": "^0.6.0",
    "wext-manifest": "^2.2.0",
    "write-webpack-plugin": "^1.1.0",
    "zip-webpack-plugin": "^4.0"
  },
  "dependencies": {
    "webextension-polyfill": "^0.9.0"
  }
}
